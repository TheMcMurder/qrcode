{
  "name": "@qrcode/web-example",
  "version": "1.0.0",
  "description": "Web example for QR code generator",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:gh-pages": "NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "pnpm exec prettier . --write",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "keywords": [
    "qrcode",
    "wasm",
    "rust"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@qrcode/wasm": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
